{
  "theme": "Atom One",
  "selectedAuthType": "oauth-personal",
  "sandbox": false,
  "checkpointing": {
    "enabled": true
  },
  "allowMCPServers": [
    "context7",
    "k8s-mcp"
  ],
  "mcpServers": {
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ]
    },
    "k8s-mcp": {
      "command": "npx",
      "args": [
        "-y",
        "kubernetes-mcp-server@latest",
        "--read-only"
      ]
    }
  }
}